  "scripts": {
    "dev:client": "cross-env PUBLIC_PATH=https://localhost:1234/ cross-env NODE_ENV=development webpack-dev-server --config ./app/webpack/client.config.js --progress",
    "dev:server": "cross-env PUBLIC_PATH=https://localhost:1234/ cross-env PORT=12345 cross-env NODE_ENV=development babel-node --icu-data-dir=node_modules/full-icu --presets @babel/env app/webpack/index.js",
    "build:client": "cross-env BABEL_ENV=client cross-env PWA=false webpack --config ./app/webpack/client.config.js",
    "build:server": "cross-env BABEL_ENV=server webpack --config ./app/webpack/server.config.js",
    "build:all": "concurrently \"yarn build:client\" \"yarn build:server\"",
    "package": "node etc/scripts/package.js",
    "build": "rimraf reports && rimraf dist/i18n && yarn lint && yarn type-check && yarn coverage && yarn build:all",
    "build:run": "cross-env PORT=12345 node --icu-data-dir=node_modules/full-icu dist/server/server.js",
    "prebuild:prod": "yarn version --patch --no-git-tag-version --preid dev",
    "build:prod": "yarn build && yarn translations:manage --gen && yarn package",
    "postbuild:prod": "yarn version --prerelease --no-git-tag-version --preid dev",
    "lint:css": "stylelint './app/**/*.(js|jsx|ts|tsx)'",
    "lint:js": "./node_modules/eslint/bin/eslint.js ./ --fix --ext .jsx,.js,.ts,.tsx",
    "lint:jsonly": "./node_modules/eslint/bin/eslint.js ./ --fix --ext .jsx,.js",
    "lint": "yarn lint:css && yarn lint:js",
    "type-check": "tsc --noEmit --skipLibCheck",
    "test": "jest",
    "coverage": "jest --collectCoverage",
    "prestart": "yarn install --production",
    "start": "yarn build:run",
    "version": "echo \"versioning $npm_package_version\"",
    "stats:dups": "webpack-stats-duplicates ./reports/client-stats.json && webpack-stats-duplicates ./reports/server-stats.json",
    "translations:manage": "babel-node --presets @babel/env ./etc/i18n/manageTranslations.js",
    "translations:update": "babel-node --presets @babel/env ./etc/i18n/updateTranslation.js"
  },
  "engines": {
    "node": ">=10",
    "yarn": "1.19.x"
  },
  "engineStrict": true
