    "lodash": "^4.17.15",
    "object-path-immutable": "^3.0.2",
    "react": ">=15",
    "react-dom": "^16.9.0",
    "react-helmet-async": "^1.0.3",
    "react-imported-component": "^6.1.0",
    "react-in-viewport": "^1.0.0-alpha.9",
    "react-redux": "^7.1.1",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-toastify": "^5.4.0",
    "redux": "^4.0.4",
    "styled-components": "^4.3.2",
    "typesafe-actions": "^4.4.2",
    "typescript": "^3.6.3",
    "uuid": "^3.3.3",
    "web": "^0.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/polyfill": "^7.6.0",
    "@types/is-promise": "^2.1.0",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.138",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.2",
    "@types/styled-components": "^4.1.19",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "babel-plugin-macros": "^2.6.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-styled-components": "^1.10.6",
    "cheerio": "^1.0.0-rc.3",
    "concurrently": "^4.1.2",
    "cross-env": "^6.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^6.4.0",
    "eslint-plugin-react": "^7.14.3",
    "express": "^4.17.1",
    "jest": "^24.9.0",
    "llog": "^0.2.0",
    "nodemon": "^1.19.2",
    "parcel-bundler": "^1.12.3",
    "pino": "^5.13.3",
    "react-hot-loader": "^4.12.13",
    "rimraf": "^3.0.0",
    "stylelint": "^11.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "through": "^2.3.8",
    "ts-jest": "^24.1.0"
  },
  "scripts": {
    "dev": "yarn generate-imported-components && parcel app/index.html",
    "dev:server": "nodemon -e js,jsx,html,ts,tsx --ignore dist/ --ignore app/imported.js --exec 'yarn build && yarn start'",
    "build": "rimraf dist && yarn generate-imported-components && yarn create-bundles",
    "start": "node dist/server",
    "create-bundles": "concurrently \"yarn create-bundle:client\" \"yarn create-bundle:server\"",
    "create-bundle:client": "cross-env BABEL_ENV=client parcel build app/index.html -d dist/client --public-url /dist/client",
    "create-bundle:server": "cross-env BABEL_ENV=server parcel build server/index.js -d dist/server --public-url /dist --target=node",
    "generate-imported-components": "imported-components app app/imported.js",
    "lint:css": "stylelint './app/**/*.(js|jsx|ts|tsx)'",
    "lint:js": "./node_modules/eslint/bin/eslint.js ./ --ext .jsx,.js,.ts,.tsx",
    "lint": "yarn lint:css && yarn lint:js"
  }
}
