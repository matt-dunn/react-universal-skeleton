  "scripts": {
    "dev:client": "cross-env PUBLIC_PATH=https://0.0.0.0:1234/ cross-env NODE_ENV=development webpack-dev-server --config ./app/webpack/client.config.js --progress",
    "dev:server": "cross-env PUBLIC_PATH=https://0.0.0.0:1234/ cross-env PORT=12345 cross-env NODE_ENV=development babel-node --presets @babel/env app/webpack/index.js",
    "build:client": "webpack --config ./app/webpack/client.config.js",
    "build:server": "webpack --config ./app/webpack/server.config.js",
    "build:all": "rimraf dist && concurrently \"yarn build:client\" \"yarn build:server\"",
    "package": "cross-env TARGET=dist node etc/scripts/package.js",
    "build": "rimraf reports && yarn lint && yarn type-check && yarn coverage && yarn build:all",
    "build:run": "cross-env PORT=12345 node dist/server/server.js",
    "prebuild:prod": "yarn version --patch --no-git-tag-version --preid dev",
    "build:prod": "yarn build && yarn package",
    "postbuild:prod": "yarn version --prerelease --no-git-tag-version --preid dev",
    "lint:css": "stylelint './app/**/*.(js|jsx|ts|tsx)'",
    "lint:js": "./node_modules/eslint/bin/eslint.js ./ --fix --ext .jsx,.js,.ts,.tsx",
    "lint:jsonly": "./node_modules/eslint/bin/eslint.js ./ --fix --ext .jsx,.js",
    "lint": "yarn lint:css && yarn lint:js",
    "type-check": "tsc --noEmit --skipLibCheck",
    "test": "jest",
    "coverage": "jest --collectCoverage",
    "prestart": "yarn install --production",
    "start": "yarn build:run",
    "version": "echo \"versioning $npm_package_version\""
